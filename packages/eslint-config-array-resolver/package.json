{
  "name": "@sushichan044/eslint-config-array-resolver",
  "description": "Resolves flat config module. For internal use only.",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "author": {
    "name": "sushichan044",
    "url": "https://github.com/sushichan044"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushichan044/eslint-toolbox.git",
    "directory": "packages/eslint-config-array-resolver"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "build": "tsdown",
    "build:check": "pnpm run build && pnpm pack && attw"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "catalog:biome",
    "@types/node": "24.1.0",
    "@typescript-eslint/utils": "8.38.0",
    "@virtual-live-lab/eslint-config": "catalog:eslint",
    "@virtual-live-lab/tsconfig": "catalog:typescript",
    "eslint": "catalog:eslint",
    "fs-fixture": "catalog:test",
    "publint": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:typescript",
    "typescript-eslint": "catalog:eslint",
    "unplugin-unused": "catalog:build",
    "vitest": "catalog:test"
  },
  "dependencies": {
    "bundle-require": "^5.1.0",
    "empathic": "^2.0.0",
    "mlly": "^1.7.4",
    "pathe": "^2.0.3"
  }
}
