{
  "name": "eslint-rule-explorer",
  "version": "0.2.0",
  "description": "A powerful CLI tool for exploring and analyzing ESLint rules in your projects.",
  "license": "Apache-2.0",
  "author": {
    "name": "sushichan044",
    "url": "https://github.com/sushichan044"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushichan044/eslint-toolbox.git",
    "directory": "packages/eslint-rule-explorer"
  },
  "bin": {
    "eslint-rule-explorer": "bin/eslint-rule-explorer.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix .",
    "format": "oxfmt",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "build": "tsdown"
  },
  "dependencies": {
    "@sushichan044/eslint-config-array-resolver": "workspace:^",
    "gunshi": "catalog:prod"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "catalog:dev",
    "@types/node": "catalog:dev",
    "@typescript-eslint/utils": "catalog:prod",
    "@virtual-live-lab/eslint-config": "catalog:dev",
    "@virtual-live-lab/tsconfig": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-react": "catalog:dev",
    "fs-fixture": "catalog:dev",
    "globals": "catalog:dev",
    "oxfmt": "catalog:dev",
    "publint": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "typescript-eslint": "catalog:prod",
    "unplugin-unused": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  }
}
