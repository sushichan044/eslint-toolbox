{
  "name": "@sushichan044/eslint-config-info",
  "description": "A CLI to get information about ESLint configurations.",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "bin": {
    "eslint-config-info": "bin/eslint-config-info.mjs"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "build": "tsdown",
    "build:check": "pnpm run build && pnpm pack && attw"
  },
  "dependencies": {
    "@sushichan044/eslint-config-resolver": "workspace:^",
    "gunshi": "^0.27.0-alpha.8"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "catalog:biome",
    "@eslint/core": "0.15.1",
    "@types/node": "24.1.0",
    "@typescript-eslint/utils": "^8.27.0",
    "@virtual-live-lab/eslint-config": "catalog:eslint",
    "@virtual-live-lab/tsconfig": "catalog:typescript",
    "eslint": "catalog:eslint",
    "fs-fixture": "catalog:test",
    "publint": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:typescript",
    "typescript-eslint": "catalog:eslint",
    "unplugin-unused": "catalog:build",
    "vitest": "catalog:test"
  }
}
